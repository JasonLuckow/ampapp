{
  "auth": {},
  "api": {
    "notesapp": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 7,
              "description": "demo"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    },
    "APITesting": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "APILambdaTest",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "swag",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "storage": {
    "imagestorage": {
      "service": "S3",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "APILambdaTest": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "swag": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  }
}